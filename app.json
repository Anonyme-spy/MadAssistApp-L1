{
  "expo": {
    "name": "MadAssistant",
    "slug": "MadAssistant",
    "version": "0.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/3.png",
    "scheme": "madassistant",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/4.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a72c9"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/2.png",
        "backgroundColor": "#1a72c9"
      },
      "edgeToEdgeEnabled": false,
      "package": "com.madassistant.app",
      "versionCode": 1,
      "permissions": [
        "READ_CONTACTS",
        "CALL_PHONE",
        "READ_PHONE_STATE",
        "CALL_PRIVILEGED"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/2.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c6a770a6-478c-495c-bec1-d1cdbac5f265"
      }
    },
    "owner": "anonymespy"
  }
}
